{
  "name": "mein-gehirn",
  "version": "1.0.0",
  "private": true,
  "description": "mein-gehirn",
  "author": "_brln.by",
  "keywords": [
    "gatsby"
  ],
  "scripts": {
    "dev": "gatsby develop",
    "start": "gatsby develop",
    "build": "gatsby build",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "typecheck": "tsc --noEmit",
    "format:check": "prettier --check \"src/**/*.{js,jsx,ts,tsx,md,mdx}\"",
    "format:fix": "prettier --write \"src/**/*.{js,jsx,ts,tsx,md,mdx}\"",
    "configure-husky": "npx husky install && npx husky add .husky/pre-commit \"npx --no-install lint-staged\""
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*.{js,jsx,json}": [
      "prettier --write \"src/**/*.{ts,tsx}\""
    ]
  },
  "dependencies": {
    "@headlessui/react": "^1.7.7",
    "@mdx-js/react": "^2.2.1",
    "aos": "^2.3.4",
    "gatsby": "^5.4.1",
    "gatsby-plugin-google-gtag": "^5.4.0",
    "gatsby-plugin-image": "^3.4.0",
    "gatsby-plugin-manifest": "^5.4.0",
    "gatsby-plugin-mdx": "^5.3.1",
    "gatsby-plugin-nprogress": "^5.4.0",
    "gatsby-plugin-sharp": "^5.4.0",
    "gatsby-plugin-sitemap": "^6.4.0",
    "gatsby-source-filesystem": "^5.3.1",
    "gatsby-transformer-sharp": "^5.4.0",
    "gsap": "^3.11.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.7.1",
    "styled-components": "^5.3.6",
    "three": "^0.148.0"
  },
  "devDependencies": {
    "@types/aos": "^3.0.4",
    "@types/node": "^18.11.18",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.10",
    "@types/styled-components": "^5.1.26",
    "@types/three": "^0.148.0",
    "@typescript-eslint/eslint-plugin": "^5.48.2",
    "@typescript-eslint/parser": "^5.48.2",
    "autoprefixer": "^10.4.13",
    "eslint": "^8.32.0",
    "eslint-config-google": "^0.14.0",
    "eslint-plugin-react": "^7.32.1",
    "gatsby-plugin-postcss": "^6.5.0",
    "gatsby-plugin-root-import": "^2.0.9",
    "husky": "^8.0.3",
    "lint-staged": "^13.1.0",
    "postcss": "^8.4.21",
    "prettier": "^2.8.3",
    "tailwindcss": "^3.2.4",
    "typescript": "^4.9.4"
  }
}
